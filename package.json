{
  "name": "todo-list",
  "version": "0.0.4",
  "description": "Find TODOs, FIXMEs, and NOTEs in your project. Sort by assignee or file.",
  "author": "Varun Ramesh <varunramesh@live.com>",
  "license": "MIT",
  "keywords": [
    "todo",
    "fixme",
    "note"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rameshvarun/todo-list.git"
  },
  "dependencies": {
    "exec-queue": ">=0.0.2",
    "fs-walk": ">=0.0.2",
    "gaze": ">=1.1.3",
    "mocha": ">=3.2.0",
    "opn": ">=5.5.0",
    "slug": ">=4.0.2",
    "snyk": ">=1.452.0",
    "swig": ">=0.14.0",
    "underscore": ">=1.12.1"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">= 0.8.0"
  },
  "bin": {
    "todo-list": "./bin/todo-list"
  },
  "scripts": {
    "start": "cd lib && node server.js",
    "test": "node node_modules/mocha/bin/mocha -c -S -R spec",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "./lib"
  },
  "devDependencies": {
    "mocha": ">=8.3.0"
  },
  "snyk": true
}
